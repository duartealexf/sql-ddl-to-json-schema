{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Compilation",
      "args": ["lib/mysql/parser/grammar.ne"],
      "program": "${workspaceFolder}/node_modules/nearley/bin/nearleyc.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Opened Test File",
      "env": {
        "DEBUG": "*",
        "TS_NODE_PROJECT": "${workspaceRoot}/test/tsconfig.json"
      },
      "runtimeArgs": ["-r", "ts-node/register"],
      "program": "${workspaceRoot}/node_modules/ava/profile.js",
      "args": ["${file}"],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Update Expectation Of Opened Test File",
      "env": {
        "DEBUG": "*",
        "DRY_UPDATE": "1",
        "TS_NODE_PROJECT": "${workspaceRoot}/test/tsconfig.json"
      },
      "runtimeArgs": ["-r", "ts-node/register"],
      "program": "${workspaceRoot}/node_modules/ava/profile.js",
      "args": ["${file}"],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
